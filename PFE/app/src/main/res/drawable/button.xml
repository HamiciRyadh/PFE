<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="200dp" />
            <stroke android:width="1dip" android:color="#E9BC62" />
            <gradient android:angle="-90" android:startColor="#fabb73" android:endColor="#facd73"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="200dp" />
            <stroke android:width="1dip" android:color="#facd73" />
            <solid android:color="#E9BC62"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="200dp" />
            <stroke android:width="1dip" android:color="#E9BC62" />
            <gradient android:angle="-90" android:startColor="#fabb73" android:endColor="#facd73" />
        </shape>
    </item>
</selector>